{
  "name": "express-auth-demo",
  "version": "0.1",
  "description": "login demo with Express.js and Vue.js",
  "repository": "https://github.com/Arondight/express-auth-demo.git",
  "homepage": "https://github.com/Arondight/express-auth-demo",
  "bugs": "https://github.com/Arondight/express-auth-demo.git/issues",
  "author": "Qin Fandong",
  "license": "MIT",
  "type": "module",
  "exports": "./app.js",
  "keywords": [
    "express",
    "login",
    "vue"
  ],
  "engines": {
    "node": ">16.0.0"
  },
  "imports": {
    "#@server/*": "./server/*/index.js",
    "#@src/*": "./src/*/index.js",
    "#server/*": "./server/*.js",
    "#src/*": "./src/*.js"
  },
  "scripts": {
    "build": "npx vite build",
    "start": "node app.js",
    "serve": "npx vite serve",
    "check": "npx eslint ./*.js ./*.html ./config/ ./podman-compose/ ./server/ ./src/",
    "format": "npx prettier --config ./.prettierrc.json -uw ./*.md ./*.js ./.*.json ./*.json ./*.html ./config/ ./podman-compose/ ./server/ ./src/",
    "update": "npx ncu -u; npm update"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "4.2.3",
    "axios": "1.7.4",
    "body-parser": "1.20.3",
    "compression": "1.7.4",
    "connect-history-api-fallback": "2.0.0",
    "connect-mongodb-session": "3.1.1",
    "cors": "2.8.5",
    "crypto-js": "4.2.0",
    "element-plus": "2.3.8",
    "express": "4.21.2",
    "express-session": "1.18.1",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "mongoose": "8.9.5",
    "morgan": "1.10.0",
    "unplugin-auto-import": "0.16.6",
    "unplugin-vue-components": "0.25.1",
    "vite": "4.5.9",
    "vue": "3.3.4",
    "vue-router": "4.2.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.2.1",
    "eslint": "8.45.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-sort-exports": "0.8.0",
    "eslint-plugin-vue": "9.15.1",
    "eslint-plugin-yml": "1.8.0",
    "npm-check-updates": "16.10.17",
    "prettier": "3.0.0"
  }
}
